{
  "name": "translate.js",
  "version": "0.7.1",
  "description": "Javascript micro library for translations (i18n) with support for placeholders and multiple plural forms.",
  "devDependencies": {
    "benchmark": "2.1.4",
    "expect.js": "0.3.1",
    "mocha": "5.1.1",
    "onchange": "3.3.0",
    "rollup": "0.58.1",
    "rollup-plugin-buble": "0.19.2"
  },
  "scripts": {
    "build:cjs": "rollup -c --o dist/index.js -f cjs",
    "build:browser": "rollup -c --o dist/translate.js -f umd --name \"translatejs\"",
    "build": "npm run build:cjs && npm run build:browser",
    "_test": "node_modules/mocha/bin/mocha test.js",
    "test": "npm run build:cjs && npm run _test",
    "dev": "onchange \"./index.js\" -- npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:StephanHoyer/translate.js.git"
  },
  "keywords": [
    "translation",
    "translate",
    "i18n",
    "js",
    "javascript",
    "micro",
    "library"
  ],
  "author": {
    "name": "Jonas Girnatis",
    "email": "dermusterknabe@gmail.com"
  },
  "module": "index.js",
  "main": "dist/index.js",
  "browser": "dist/translate.js",
  "contributors": [
    {
      "name": "Stephan Hoyer",
      "email": "ste.hoyer@gmail.com"
    },
    {
      "name": "Már Örlygsson",
      "url": "http://mar.anomy.net/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/musterknabe/translate.js/issues"
  },
  "homepage": "https://github.com/musterknabe/translate.js"
}
